{
  "version": 2,
  "builds": [
    {
      "src": "config/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "50mb" }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "config/wsgi.py" }, ]
    { "src": "/static/(.*)", "dest": "/static/$1" }, 
    { "src": "/media/(.*)", "dest": "/media/$1" }, 
}

